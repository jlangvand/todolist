<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="taskCellPane" minHeight="-Infinity" minWidth="300.0"
            styleClass="taskBackground" stylesheets="@../css/TaskPage.css" xmlns="http://javafx.com/javafx/15.0.1">
    <children>
        <Label fx:id="cellTitle" layoutX="14.0" layoutY="2.0" styleClass="normalLabel" text="Title" textFill="WHITE"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <ImageView fx:id="cellStatusImage" fitHeight="48.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <image>
                <Image url="@../images/not_Done.png"/>
            </image>
        </ImageView>
        <Label fx:id="cellDate" layoutX="14.0" layoutY="30.0" text="Date" textFill="WHITE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
    </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
